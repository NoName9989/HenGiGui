--SCRIPT1--
--Anti ServerHop--
-- Gui nhỏ bật/tắt Anti Teleport + nút đóng
local Players = game:GetService("Players")
local TeleportService = game:GetService("TeleportService")
local player = Players.LocalPlayer

-- Tạo ScreenGui
local ScreenGui = Instance.new("ScreenGui")
ScreenGui.Name = "AntiTeleportUI"
ScreenGui.Parent = player:WaitForChild("PlayerGui")

-- Tạo Frame chính
local Frame = Instance.new("Frame")
Frame.Size = UDim2.new(0, 200, 0, 80)
Frame.Position = UDim2.new(0, 20, 0, 200)
Frame.BackgroundColor3 = Color3.fromRGB(40, 40, 40)
Frame.Parent = ScreenGui

-- Nút Toggle Anti Teleport
local ToggleButton = Instance.new("TextButton")
ToggleButton.Size = UDim2.new(0, 150, 0, 40)
ToggleButton.Position = UDim2.new(0, 25, 0, 10)
ToggleButton.BackgroundColor3 = Color3.fromRGB(200, 50, 50)
ToggleButton.TextColor3 = Color3.fromRGB(255, 255, 255)
ToggleButton.TextScaled = true
ToggleButton.Text = "Anti Teleport: OFF"
ToggleButton.Parent = Frame

-- Nút Close
local CloseButton = Instance.new("TextButton")
CloseButton.Size = UDim2.new(0, 25, 0, 25)
CloseButton.Position = UDim2.new(1, -30, 0, 5)
CloseButton.BackgroundColor3 = Color3.fromRGB(150, 0, 0)
CloseButton.TextColor3 = Color3.fromRGB(255, 255, 255)
CloseButton.Text = "X"
CloseButton.Parent = Frame

-- Biến quản lý
local originalFuncs = {}
local hooked = false

-- Hàm bật/tắt
local function toggleAntiTeleport()
	if not hooked then
		local funcs = {
			"Teleport",
			"TeleportAsync",
			"TeleportPartyAsync",
			"TeleportToPlaceInstance",
			"TeleportToPrivateServer"
		}
		for _, fname in ipairs(funcs) do
			if typeof(TeleportService[fname]) == "function" then
				originalFuncs[fname] = TeleportService[fname]
				hookfunction(TeleportService[fname], function(...)
					warn("[BLOCKED] "..fname.." attempt blocked!")
					return
				end)
			end
		end
		hooked = true
		ToggleButton.BackgroundColor3 = Color3.fromRGB(50, 200, 50)
		ToggleButton.Text = "Anti Teleport: ON"
	else
		for fname, func in pairs(originalFuncs) do
			hookfunction(TeleportService[fname], func)
		end
		hooked = false
		ToggleButton.BackgroundColor3 = Color3.fromRGB(200, 50, 50)
		ToggleButton.Text = "Anti Teleport: OFF"
	end
end

-- Sự kiện click
ToggleButton.MouseButton1Click:Connect(toggleAntiTeleport)

-- Sự kiện đóng frame
CloseButton.MouseButton1Click:Connect(function()
	ScreenGui:Destroy()
end)
